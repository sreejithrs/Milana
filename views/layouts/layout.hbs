      document.querySelector("body").classList.toggle("active");
    })
  </script>

  <script>
    $('#Category').on('change', function () {
      $('#subCat').show()
      let cat = this.value
      $.ajax({
        type: "get",
        url: '/admin/getsub',
        data: {
          "cat": cat,
        },
        success: function (subCat) {

          $('#subCat')[0].options.length = 0;

          var subArray = subCat[0].SubCategory;
          subArray.forEach(elem => {
            $('#subCat').append($('<option>',
              {
                value: elem,
                text: elem
              }));
          })
        },
        error: function (error) {
          
        }
      });
    });
  </script>



